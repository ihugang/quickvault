{
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=QuickVaultOcr;User Id=sa;Password=YourPassword123!;TrustServerCertificate=True;",
        "Redis": "localhost:6379"
    },
    "OpenAI": {
        "ApiKey": "sk-your-openai-api-key-here",
        "ApiBase": "https://api.openai.com/v1"
    },
    "Azure": {
        "OpenAI": {
            "Endpoint": "https://your-resource.openai.azure.com/",
            "ApiKey": "your-azure-openai-key",
            "DeploymentName": "gpt-4"
        }
    },
    "Authentication": {
        "ApiKey": "your-secret-api-key-change-in-production"
    },
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "GeneralRules": [
            {
                "Endpoint": "POST:/api/v1/ocr/analyze",
                "Period": "1m",
                "Limit": 60
            },
            {
                "Endpoint": "POST:/api/v1/ocr/analyze",
                "Period": "1h",
                "Limit": 1000
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
